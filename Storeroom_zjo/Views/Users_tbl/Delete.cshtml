@model Storeroom_zjo.Models.Users_tbl

@{
    ViewBag.Title = "Delete";
}

<h2>Usuń</h2>

<h3>Czy napewno chcesz usunąć tego użytkownika?</h3>
<div>
    <h4></h4>
    <hr />
    <dl class="dl-horizontal">
        <dt>
            Nazwa Użytkownika
        </dt>

        <dd>
            @Html.DisplayFor(model => model.UserName)
        </dd>

        <dt>
            Dostęp
        </dt>

        <dd>
            @Html.DisplayFor(model => model.Permission)
        </dd>

        <dt>
            Stanowisko
        </dt>

        <dd>
            @Html.DisplayFor(model => model.Positions_tbl.Position)
        </dd>

    </dl>

    @using (Html.BeginForm()) {
        @Html.AntiForgeryToken()

        <div class="form-actions no-color">
            <input type="submit" value="Usuń" class="btn btn-danger" /> |
            @Html.ActionLink("Powrót do listy użytkowników ", "Index", null, new { @class = "btn btn-primary" })
        </div>
    }
</div>
